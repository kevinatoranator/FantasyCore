<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<!-- Oak Log -->
<ThingDef ParentName="ResourceBase">
    <defName>OakLog</defName>
    <label>oak log</label>
    <description>A log from an oak tree.</description>
    <graphicData>
      <texPath>Things/Item/Resource/OakLog</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>50</stackLimit>
    <statBases>
      <MaxHitPoints>70</MaxHitPoints>
      <MarketValue>32</MarketValue>
      <Mass>0.6</Mass>
      <Flammability>0.6</Flammability> <!-- higher -->
			<DeteriorationRate>2.0</DeteriorationRate> <!-- adjust-->
		</statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Items</li> <!-- Maybe add some new ones/change-->
    </thingCategories>
  </ThingDef>
  
