<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
<!-- Copper Ingot -->
<ThingDef ParentName="ResourceBase">
    <defName>CopperIngot</defName>
    <label>copper ingot</label>
    <description>An ingot of copper.</description>
    <graphicData>
      <texPath>Things/Item/Resource/CopperIngot</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>50</stackLimit>
    <statBases>
      <MaxHitPoints>70</MaxHitPoints>
      <MarketValue>32</MarketValue>
      <Mass>0.6</Mass>
      <Flammability>0.6</Flammability> <!-- none? -->
			<DeteriorationRate>2.0</DeteriorationRate> <!-- Remove -->
		</statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Items</li> <!-- Maybe add some new ones-->
    </thingCategories>
  </ThingDef>

